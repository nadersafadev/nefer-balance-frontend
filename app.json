{
  "expo": {
    "name": "equa-wallet",
    "slug": "equa-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Equa-Icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Equa-SplashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nader-safa.nefer-balance"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Equa-Icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nader.safa.dev.neferbalance"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Equa-Icon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-bottom-tabs",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f20f1523-331d-466c-93e8-5603108cdf3a"
      }
    }
  }
}
